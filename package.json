{
  "name": "react-hello",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.1.2",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "antd": "^4.15.5",
    "axios": "^0.21.1",
    "craco-less": "^1.17.1",
    "echarts": "^5.2.2",
    "echarts-for-react": "^3.0.2",
    "mobx": "^6.3.0",
    "mobx-react-lite": "^3.2.0",
    "qs": "^6.5.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "swr": "^0.5.6",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^6.4.1",
    "cross-env": "^7.0.3",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "mocker-api": "^2.8.3",
    "mockjs": "^1.1.0",
    "prettier": "^2.3.0"
  },
  "scripts": {
    "start": "craco start",
    "start-mock": "cross-env API_ENV=mock craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "img-to-webp": "find ./src -name '*.png' -o -name '*.jpeg' | xargs -I img cwebp img -o img.webp",
    "gif-to-webp": "find ./src -name '*.gif' | xargs -I img gif2webp img -o img.webp",
    "webp": "npm run img-to-webp && npm run gif-to-webp"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint",
    "*.{js,jsx,html,css}": "prettier --write"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-const-assign": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
